# Copy this file to .env and fill in real values

# Database connection
DATABASE_URL=postgresql://postgres:pass@localhost:5432/acra

# Data source
API_URL=https://data.gov.sg/api/action/datastore_search
RESOURCE_ID=<dataset_id>
PAGE_SIZE=100

# Local dev toggles
# Set to 1 to skip ingestion on startup / scheduler
DISABLE_STARTUP_INGEST=1
DISABLE_SCHEDULER=1
